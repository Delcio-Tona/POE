<Window x:Class="POE.Window4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POE"
        mc:Ignorable="d"
        Title="Window4" Height="450" Width="800">
    <Grid>
        <Label Content="Claim to verify/Approve" HorizontalAlignment="Left" Margin="12,14,0,0" VerticalAlignment="Top" FontSize="24" Width="281" FontWeight="Bold"/>
        <Button Content="Verify Claim" HorizontalAlignment="Left" Margin="79,365,0,0" VerticalAlignment="Top" FontSize="18" Width="110"/>
        <Button Content="Approve Claim" HorizontalAlignment="Left" Margin="212,364,0,0" VerticalAlignment="Top" FontSize="18"/>
        <TextBox HorizontalAlignment="Left" Margin="-2,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="781" Height="2" RenderTransformOrigin="0.5,0.5">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleY="-1"/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>



    </Grid>
</Window>
